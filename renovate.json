{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "build-images.sh"
            ],
            "matchStrings": [
                "ghcr\\.io\/nethesis\/nethvoice-cti:(?<currentValue>[^\\s]+)"
            ],
            "depNameTemplate": "nethesis/nethvoice-cti",
            "datasourceTemplate": "github-releases"
        }
    ],
    "packageRules": [
        {
            "matchPackageNames": [
                "nethvoice-cti"
            ]
        }
    ]
}