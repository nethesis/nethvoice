{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "nethvoice defaults",
  "$id": "http://nethserver.org/json-schema/task/output/nethvoice/get-defaults",
  "description": "Get nethvoice default configuration values",
  "examples": [
    {
      "accepted_timezone_list": [
        "Europe/Amsterdam",
        "Europe/Berlin",
        "Europe/Brussels",
        "Europe/London"
      ]
    }
  ],
  "type": "object",
  "additionalProperties": false,
  "required": ["accepted_timezone_list"],
  "properties": {
    "accepted_timezone_list": {
      "description": "List of accepted timezones",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
